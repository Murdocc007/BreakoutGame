<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aditya.breakoutgame" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".BreakoutGame"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ScoreEntry"
            android:label="@string/title_activity_score_entry"
            android:theme="@style/AppTheme.NoActionBar" >

        </activity>
        <activity
            android:name=".HallOfFame"
            android:label="@string/title_activity_hall_of_fame"
            android:theme="@style/AppTheme.NoActionBar" >


        </activity>
    </application>

</manifest>
